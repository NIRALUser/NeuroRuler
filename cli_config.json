// Default settings for CLI. Can be overriden by command-line arguments.
// Comments beginning with // are ignored.
// Use "True" or "False" for binary options.
// For other field types, the comments specify what the allowed values are.
// If you're a user, do not remove fields.
// If you're a developer who's adding or removing options, modify parser.py.
{
    "DEBUG": "False",
    "RAW": "False",
    "X": 0,
    "Y": 0,
    "Z": 0,
    // -1 means NeuroRuler will use the middle slice
    "SLICE": -1,
    "CONDUCTANCE": 3.0,
    "SMOOTHING": 5,
    "TIME_STEP": 0.0625,
    // THRESHOLD_FILTER can be "Otsu" or "Binary"
    "THRESHOLD_FILTER": "Otsu",
    // Binary threshold filter uses lower and upper threshold values.
    // Otsu ignores them (automatically calculates threshold values).
    "LOWER_BINARY_THRESHOLD": 0.0,
    "UPPER_BINARY_THRESHOLD": 200.0
}
